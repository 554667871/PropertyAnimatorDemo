<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.flyou.propertyanimator">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ObjectAnimatorActivity" />
        <activity android:name=".ValueAnimatorActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".interpolator.InterpolatorActivity" />
        <activity android:name=".interpolator.InterpolatorPathActivity" />
        <activity android:name=".EvaluatorActivity" />
        <activity android:name=".customProperty.CustomPropertyActivity" />
        <activity android:name=".AnimActivity"></activity>
    </application>

</manifest>